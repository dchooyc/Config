# Git stuff
alias gi="git init"
alias gs="git status"
alias gc="git commit"
alias gcm="gc -m"
alias ga="git add"
alias gr="git restore"
alias grs="gr --staged"
alias gst="git stash"
alias gsta="gst apply"

# Branches
alias gb="git branch"
alias gbd="gb -D"
alias gbc="git checkout"
alias gbn="gbc -b"

# Push
alias gp="git push"
alias gpo="gp origin"
alias gpod="gpo --delete"
alias gpso="gp --set-upstream origin"

# Retrieve
alias clone="git clone"
alias pull="git pull"

# Config
alias gconfg="git config --global"
