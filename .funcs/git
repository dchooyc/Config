# soft reset
gsoft() {
  git reset --soft HEAD~$1
}

# hard reset
ghard() {
  git reset --hard HEAD~$1
}

# clone from repo
gd () {
  git clone "git@github.com:dchooyc/$1.git"
}

# push to new repo
gnew () {
  git remote add origin git@github.com:dchooyc/$1.git
  git branch -M master
  git push -u origin master
}
